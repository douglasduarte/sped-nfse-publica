{
    "name" : "douglasduarte/sped-nfse-publica",
    "type" : "library",
    "description" : "Biblioteca para integração NFSe Modelo Publica.",
    "keywords" : [
        "nfe",
        "nfse",
        "sped",
        "nfephp"
    ],
    "homepage" : "https://github.com/nfephp-org/sped-nfse-publica",
    "license" : [
        "LGPL-3.0-or-later",
	"MIT"
    ],
    "authors" : [
        {
            "name": "Roberto L. Machado",
            "email": "linux.rlm@gmail.com.br",
            "homepage": "http://www.nfephp.com.br",
            "role": "Developer"
        },
        {
            "name" : "Claudemir Todo Bom",
            "email" : "claudemir@todobom.com",
            "homepage" : "https://todobom.com",
            "role" : "Developer"
        }
    ],
    "require" : {
        "php" : ">= 7.1",
        "nfephp-org/sped-common" : "^5.0",
        "justinrainbow/json-schema" : "^5.2",
        "ext-zlib" : "*",
        "ext-dom" : "*",
        "ext-openssl" : "*",
        "ext-json" : "*",
        "ext-simplexml" : "*",
        "ext-libxml" : "*"
    },
    "require-dev" : {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.1",
        "scrutinizer/ocular": "^1.3",
        "phpstan/phpstan": "^0.12",
        "nunomaduro/phpinsights": "^1.14"
    },
    "autoload" : {
        "psr-4" : {
            "NFePHP\\NFSePublica\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "NFePHP\\NFSePublica\\Tests\\" : "tests/"
        }
    },
    "scripts" : {
        "test" : "phpunit -c phpunit.xml.dist",
        "phpcbf" : "vendor/bin/phpcbf --standard=psr2 src",
        "phpcs" : "vendor/bin/phpcs --standard=psr2 src",
        "phpstan" : "vendor/bin/phpstan analyse src/ --level 7"
    },
    "extra" : {
        "branch-alias" : {
            "v1.0" : "1.0-dev"
        }
    },
    "minimum-stability" : "stable"
}
